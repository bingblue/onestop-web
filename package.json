{
  "name": "onestop-web",
  "version": "0.0.1",
  "description": "一站，一站式购物",
  "author": "xiaomucool",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "axios": "*",
    "element-ui": "*",
    "vue": "*",
    "vue-awesome": "*",
    "vue-router": "*",
    "vuex": "*"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "*",
    "@vue/cli-plugin-eslint": "*",
    "@vue/cli-service": "*",
    "@vue/eslint-config-standard": "*",
    "lint-staged": "*",
    "stylus": "*",
    "stylus-loader": "*",
    "vue-template-compiler": "*"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "readme": "ERROR: No README data found!"
}
